{
  "name": "@comoi/convex",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./dataModel": "./convex/_generated/dataModel.d.ts"
  },
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy --typecheck=disable",
    "seed": "convex run seed:runSeed",
    "seed:reset": "convex run seed:runSeed '{\"reset\": true}'",
    "build": "tsc --noEmit",
    "lint": "biome check . && eslint .",
    "lint:fix": "biome check --write . && eslint . --fix",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf .turbo"
  },
  "dependencies": {
    "@comoi/shared": "workspace:*",
    "@faker-js/faker": "10.2.0",
    "convex": "1.31.6",
    "effect": "3.19.15"
  },
  "devDependencies": {
    "@comoi/eslint-config": "workspace:*",
    "@comoi/typescript-config": "workspace:*",
    "@types/node": "^25.0.10",
    "convex-test": "0.0.41",
    "eslint": "9.39.2",
    "vitest": "4.0.18"
  }
}
