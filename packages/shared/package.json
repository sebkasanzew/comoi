{
  "name": "@comoi/shared",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check . && eslint .",
    "lint:fix": "biome check --write . && eslint . --fix",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "effect": "^3.14.1"
  },
  "devDependencies": {
    "@comoi/eslint-config": "workspace:*",
    "@comoi/typescript-config": "workspace:*",
    "eslint": "^9.21.0",
    "vitest": "^3.0.6"
  }
}
